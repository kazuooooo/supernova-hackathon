<%- @users.each do |user| %>
    <%= render :partial => "user_icon", :locals => { :name => user.nick_name, :icon_path => user.icon_path } %>
<%- end %>
<div>
  <%- @keywords.each do |keyword| %>
      <li>
        <%= keyword.keys.first %>
        <%= keyword[keyword.keys.first][0] %>
        <%= keyword[keyword.keys.first][1] %>
      </li>
  <%- end %>
</div>
<table>
  <%- @shops.each_with_index do |shop, idx| %>
      <tr>
        <td>
          <table>
            <tr>
              <td> オススメ <%= idx %></td>
            </tr>
            <tr>
              <td><%= shop.name %></td>
            </tr>
            <tr>
              <td><%= shop.address %></td>
            </tr>
            <tr>
              <td<%= shop.open %></td>
            </tr>
            <tr>
              <td><%= shop.budget.average %></td>
            </tr>
            <tr>
              <td><%= link_to(shop.contact, "食べログで見る") %></td>
            </tr>
          </table>
        <td>
          <%- if shop.photo.pc.l %>
              <%= image_tag(shop.photo.pc.l) %>
          <%- end %>
        </td>
      </tr>
  <%- end %>
</table>